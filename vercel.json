{
  "version": 2,
  "builds": [
    {
      "src": "dist/spa/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "api/**.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/spa/$1"
    }
  ],
  "functions": {
    "api/**.ts": {
      "maxDuration": 10
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}